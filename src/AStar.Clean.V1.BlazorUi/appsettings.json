{
  "DetailedErrors": true,
  "Serilog": {
    "Enrich": [
      "FromLogContext",
      "WithSpan",
      "WithExceptionDetails"
    ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System.Net.Http": "Information",
        "AStar": "Debug"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Identity": "Warning",
      "Microsoft.AspNetCore.HttpLogging": "Warning",
      "CarvedRock": "Debug"
    },
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning",
        "CarvedRock": "Debug"
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Critical"
      }
    }
  },
  "AllowedHosts": "*",
  "apiConfiguration": {
    "filesApiConfiguration": {
      "baseUrl": "https://localhost:7204/"
    },
    "imagesApiConfiguration": {
      "baseUrl": "https://localhost:7008/"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=jbfiles;Trusted_Connection=True",
    "SqliteConnection": "Data Source=files.sqlite"
  },
  "databaseToUse": "SqLite", //Sql
  "ApplicationInsights": {
    "ConnectionString": "InstrumentationKey=dc879023-3390-4ca0-a507-84c2a5685023;IngestionEndpoint=https://uksouth-1.in.applicationinsights.azure.com/;LiveEndpoint=https://uksouth.livediagnostics.monitor.azure.com/"
  }
}
