{
  "Serilog": {
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithSpan",
      "WithExceptionDetails"
    ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System.Net.Http": "Information",
        "AStar": "Debug"
      }
    }
  },
  "Logging": {
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "DirectoriesList": {
    "Directories": [
      "dir1",
      "dir2"
    ]
  },
  "apiConfiguration": {
    "directories": [
      "F:\\Wallhaven",
      "F:\\LookAtNow"
    ]
  }
}
